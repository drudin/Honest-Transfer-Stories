{
  "manifest_version": 2,
  "name": "Honest Transfer Stories",
  "version": "1.0",
  "description": "An extension by David Rudin and Howler Magazine that takes the hype out of soccer transfer stories.",
  "browser_action": {
  "default_icon": "icon128.png",
  "default_title": "icon"
},
"background": {
   "scripts": ["background.js"] 
}, 
  "content_scripts": [
{
      "matches": [		
        "<all_urls>"		
     ],		
        "js": ["content.js"]		  
      }		     
    ]		    

