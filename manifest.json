{
  "manifest_version": 2,
  "name": "Honest Transfer Stories",
  "version": "1.0",
  "description": "An extension by David Rudin and Howler Magazine that takes the hype out of soccer transfer stories.",
  "browser_action": {
  "default_icon": "off.png",
  "default_title": "icon"
},
"background": {
   "scripts": ["background.js"] 
}, 
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" }
}
